{
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    },
    {
      "type": "vcs",
      "url": "https://github.com/subugoe/typo3-tmpl_fidaac"
    }
  ],
  "name": "subugoe/aac-site",
  "description": "TYPO3 AAC Website",
  "license": "GPL-2.0+",
  "type": "project",
  "require": {
    "typo3/cms": "8.7.*",
    "helhum/typo3-console": "^4.4",
    "subugoe/tmpl-fidaac": "dev-develop",
    "georgringer/news": "^6.0",
    "subugoe/pazpar2": "^6.0",
    "dmitryd/typo3-realurl": "^2.2",
    "subugoe/find": "^3.1",
    "apache-solr-for-typo3/solr": "^7.0",
    "subugoe/nkwgok": "8.x-dev"
  },
  "scripts": {
    "typo3-cms-scripts": [
      "typo3cms install:fixfolderstructure"
    ],
    "post-install-cmd": [
      "@typo3-cms-scripts"
    ],
    "post-update-cmd": [
      "@typo3-cms-scripts"
    ]
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "web"
    },
    "helhum/typo3-console": {
      "install-extension-dummy": false
    }
  }
}
